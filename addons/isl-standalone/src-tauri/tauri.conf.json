{
  "$schema": "../../node_modules/@tauri-apps/cli/schema.json",
  "package": {
    "productName": "Sapling Interactive Smartlog",
    "version": "0.1.0"
  },
  "build": {
    "devPath": "http://localhost:3000/",
    "distDir": "../dist"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.sapling-scm",
      "longDescription": "Interactive Smartlog UI for Sapling SCM",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "Interactive Smartlog UI for Sapling SCM",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": []
  }
}
